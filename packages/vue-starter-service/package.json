    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "lodash": "^4.17.11",
    "opn": "^5.4.0",
    "plop": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "start-server-webpack-plugin": "^2.2.5",
